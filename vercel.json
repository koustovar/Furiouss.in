{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Permissions-Policy",
                    "value": "otp-credentials=(self)"
                }
            ]
        }
    ]
}